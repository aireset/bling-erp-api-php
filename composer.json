{
  "name": "aireset/bling-erp-api",
  "description": "Pacote de integração com a API do Bling ERP",
  "type": "library",
  "version": "1.17.0",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Aireset\\BlingErpApi\\": "./src/",
      "Aireset\\BlingErpApi\\Entities\\Shared\\": "./src/Entities/@shared/",
      "Aireset\\BlingErpApi\\Entities\\ContasPagar\\Schema\\Shared\\": "./src/Entities/ContasPagar/Schema/@shared/",
      "Aireset\\BlingErpApi\\Entities\\ContasReceber\\Schema\\Shared\\": "./src/Entities/ContasReceber/Schema/@shared/"
    },
    "files": [
      "./src/Helpers/functions.php"
    ]
  },
  "authors": [
    {
      "name": "Alexandre Batistella Bellas",
      "email": "alexandre.bellas@gmail.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "laravel/laravel": "^10.2",
    "phpunit/phpunit": "^10.5",
    "fakerphp/faker": "^1.23"
  },
  "archive": {
    "exclude": [
      "./.github",
      "./.vscode",
      "./.editorconfig",
      "./.gitignore",
      "./CONTRIBUTING.md"
    ]
  }
}